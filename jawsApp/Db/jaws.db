record (transform, "$(P)$(JAWS)_SPALT") {
	field(DESC, "Setpoint changes to all values")
	field(CLCA, "G+E/2")
	field(CLCB, "(G-E/2)")
	field(CLCC, "H+F/2")
	field(CLCD, "(H-F/2)")
	field(CLCE, "A-B")
	field(CLCF, "C-D")
	field(CLCG, "(A+B)/2")
	field(CLCH, "(C+D)/2")
	field(CLCJ, "A")
	field(CLCK, "B")
	field(CLCL, "C")
	field(CLCM, "D")
	field(INPN, "1")
	field(OUTA, "$(P)$(JAWS)JN:SP.VAL PP MS")
	field(OUTB, "$(P)$(JAWS)JS:SP.VAL PP MS")
	field(OUTC, "$(P)$(JAWS)JE:SP.VAL PP MS")
	field(OUTD, "$(P)$(JAWS)JW:SP.VAL PP MS")
	field(OUTE, "$(P)$(JAWS)VGAP:SP.VAL PP MS")
	field(OUTF, "$(P)$(JAWS)HGAP:SP.VAL PP MS")
	field(OUTG, "$(P)$(JAWS)VCENT:SP.VAL PP MS")
	field(OUTH, "$(P)$(JAWS)HCENT:SP.VAL PP MS")
	field(OUTJ, "$(P)$(mXN).VAL PP MS")
	field(OUTK, "$(P)$(mXS).VAL PP MS")
	field(OUTL, "$(P)$(mXE).VAL PP MS")
	field(OUTM, "$(P)$(mXW).VAL PP MS")
	field(OUTN, "$(P)$(JAWS)gate.VAL PP MS")
	field(PREC, "3")
	field(INPI, "$(P)$(JAWS)COORDSYS.VAL")
	field(CMTA, "Calculated North Value")
	field(CMTB, "Calculated South Value")
	field(CMTC, "Calculated East Value")
	field(CMTD, "Calculated West Value")
	field(CMTE, "Calculated Vertical Gap")
	field(CMTF, "Calculated Vertical Center")
	field(CMTG, "Calculated Horizontal Gap")
	field(CMTH, "Calculated Horizontal Center")
	field(CMTJ, "Copy of Calculated North Value")
	field(CMTK, "Copy of Calculated South Value")
	field(CMTL, "Copy of Calculated East Value")
	field(CMTM, "Copy of Calculated West Value")
	field(CMTN, "Gate value")
	field(SDIS, "$(P)$(JAWS)_MPALT.PACT")
}

record (transform, "$(P)$(JAWS)_MPALT"){
	field(DESC, "Motor Values to Gap and Center")
	field(INPA, "$(P)$(JAWS)JN:SP:RBV PP NMS")
	field(INPB, "$(P)$(JAWS)JS:SP:RBV PP NMS")
	field(INPC, "$(P)$(JAWS)JE:SP:RBV PP NMS")
	field(INPD, "$(P)$(JAWS)JW:SP:RBV PP NMS")
	field(CLCE, "A-B")
	field(CLCF, "(A+B)/2")
	field(CLCG, "C-D")
	field(CLCH, "(C+D)/2")
	field(OUTA, "$(P)$(JAWS)JN:SP.VAL PP MS")
	field(OUTB, "$(P)$(JAWS)JS:SP.VAL PP MS")
	field(OUTC, "$(P)$(JAWS)JE:SP.VAL PP MS")
	field(OUTD, "$(P)$(JAWS)JW:SP.VAL PP MS")
	field(OUTE, "$(P)$(JAWS)VGAP:SP.VAL PP MS")
	field(OUTF, "$(P)$(JAWS)VCENT:SP.VAL PP MS")
	field(OUTG, "$(P)$(JAWS)HGAP:SP.VAL PP MS")
	field(OUTH, "$(P)$(JAWS)HCENT:SP.VAL PP MS")
	field(PREC, "3")
	field(INPI, "$(P)$(JAWS)COORDSYS.VAL")
	field(CMTA, "North Value")
	field(CMTB, "South Value")
	field(CMTC, "East Value")
	field(CMTD, "West Value")
	field(CMTE, "Calculated Vertical Gap")
	field(CMTF, "Calculated Vertical Center")
	field(CMTG, "Calculated Horizontal Gap")
	field(CMTH, "Calculated Horizontal Center")
}

record (bo, "$(P)$(JAWS)COORDSYS") {
	field(DESC, "Out from center or both in same plane")
	field(UDF, "0")
	field(DOL, "$(RELTOCENTER=0)")
}

record (fanout, "$(P)$(JAWS)_RBV:SYNC") {
	field(PINI, "1")
	field(LNK1, "$(P)$(JAWS)JN:SP:RBV.PROC NPP MS")
	field(LNK2, "$(P)$(JAWS)JS:SP:RBV.PROC NPP MS")
	field(LNK3, "$(P)$(JAWS)JE:SP:RBV.PROC NPP MS")
	field(LNK4, "$(P)$(JAWS)JW:SP:RBV.PROC NPP MS")
	field(LNK5, "$(P)$(JAWS)_MPALT.PROC PP MS")
}

record (seq, "$(P)$(JAWS)refresh") {
	field(SCAN, ".1 second")
	field(DOL1, "$(P)$(mXN).VAL NPP MS")
	field(LNK1, "$(P)$(JAWS)JN:SP:RBV NPP MS")
	field(DOL2, "$(P)$(mXS).VAL NPP MS")
	field(LNK2, "$(P)$(JAWS)JS:SP:RBV NPP MS")
	field(DOL3, "$(P)$(mXE).VAL NPP MS")
	field(LNK3, "$(P)$(JAWS)JE:SP:RBV NPP MS")
	field(DOL4, "$(P)$(mXW).VAL NPP MS")
	field(LNK4, "$(P)$(JAWS)JW:SP:RBV NPP MS")
	field(DOL5, "1")
	field(LNK5, "$(P)$(JAWS)_RBV:SYNC PP MS")
}

